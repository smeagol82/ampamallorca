{#
/**
 * @file
 * Default theme implementation for a 'form' element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}

<div id="close_search" class="close"><span class="now-ui-icons ui-1_simple-remove"></span></div>
<form{{ attributes.addClass('search-form', 'search-block-form', 'form-inline') }}>
  {{ children }}
</form>
